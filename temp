
// Assuming 'fileContents' contains the data from your file
var fileLines = fileContents.split('\n'); // Split by newlines

// Create an empty hash object
var hashObject = {};

var id = 0;
// Iterate through each line (record)
fileLines.forEach(function(line) {
    // Split the line into fields using the semicolon as the delimiter
    var fields = line.split(';');
    
    // Create an object to store the fields
    var recordObject = {};
    fields.forEach(function(field) {
        // Split each field into key and value using ':'
		var recordObjec†[name] = field[0];
		console.log(recordObjec†[name]);
		var recordObjec†[bestFo] = field[1];
		console.log(recordObjec†[bestFor]);
		var recordObjec†[features] = field[2];
		console.log(recordObjec†[features]);
		// recordObject[key] = value;
    });

    // Assuming each record has a unique identifier (e.g., 'id')
    var recordId = id + 1; // Adjust this based on your data
    hashObject[recordId] = recordObject;
});

// Now 'hashObject' contains your data with separate fields for each record
console.log(hashObject);
